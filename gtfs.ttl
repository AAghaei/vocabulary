@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix gtfs: <http://vocab.gtfs.org/terms#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

<http://vocab.gtfs.org#> a owl:Ontology ;
                         rdfs:label "General Transit Feed Specification"@en ;
                         rdfs:comment "This ontology is a translation of the General Transit Feed Specification towards URIs. For semantics of the classes and properties, see https://developers.google.com/transit/gtfs/reference."@en ;
                         rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference> ;
                         dct:creator [ foaf:mbox "Pieter.Colpaert@UGent.be"; foaf:name "Pieter Colpaert" ] ;
	                 dct:creator [ foaf:mbox "abyrd@conveyal.com"; foaf:name "Andrew Byrd" ] .

# GTFS table feed_info.txt
# https://developers.google.com/transit/gtfs/reference#feed_info_fields
gtfs:Feed rdfs:subClassOf dcat:Dataset .



# GTFS table agencies.txt https://developers.google.com/transit/gtfs/reference#agency_fields
gtfs:Agency a rdfs:Class .
# agencyId    dcterms:identifier
# agencyName  foaf:name
# agencyUrl   foaf:page
# agencyLang  language http://www.loc.gov/standards/iso639-2/php/code_list.php
# agencyPhone foaf:phone
# agencyTimezone ?:timezone (time ontology?)
gtfs:fareUrl a rdf:Property ;
             rdfs:domain gtfs:Agency ;
             rdfs:comment "http://www.w3.org/Addressing/URL/4_URI_Recommentations.html" . 


# GTFS table stops.txt https://developers.google.com/transit/gtfs/reference#stops_fields
gtfs:Stop a rdfs:Class .
# Has these predicates:
# dcterms:identifier
gtfs:code a rdf:Property ;
          rdfs:domain gtfs:Stop ;  
          rdfs:comment "See https://developers.google.com/transit/gtfs/reference" .
#foaf:name
#dct:description
#geo:long
#geo:lat
gtfs:zoneId a rdf:Property ;
            rdfs:domain gtfs:Stop ;
            rdfs:comment "Can be a literal or a URI" .
#foaf:page
#timezone

gtfs:locationType a rdf:Property ;
                  rdfs:domain gtfs:Stop .

gtfs:parentStation a rdf:Property ;
                  rdfs:domain gtfs:Stop .

#This should be in the transmodel spec and we should have an owl:sameAs 
#transmodel:StopArea rdfs:subClassOf gtfs:Stop ;
#                    rdfs:label "Stop Area" ;
#                    dct:alternate "Station" ;
#                    rdfs:comment "a Stop Area is in fact a transmodel term. A gtfs:Stop is a gtfs:StopArea when gtfs:locationType is 1"
#
#transmodel:StopPoint rdfs:subClassOf gtfs:Stop ;
#                     rdfs:label "" .
gtfs:wheelchairBoarding a rdf:Property ;
                        rdfs:domain gtfs:Stop .


# GTFS table "routes.txt"
# https://developers.google.com/transit/gtfs/reference#routes_fields
gtfs:Route a rdfs:Class .
# routeId  dcterms:identifier
# routeUrl foaf:page
# routeLongName  foaf:name
# routeShortName foaf:name
# routeDesc      dct:description
# routeColor     ?:colour
# routeTextColor ?:colour

gtfs:color a rdf:Property ;
           rdfs:range xsd:string .

gtfs:textColor a rdf:Property ;
               rdfs:range xsd:string .
# can be given to a route or a trip
gtfs:shortName a rdf:Property ;
               rdfs:subPropertyOf foaf:name .

gtfs:longName a rdf:Property ;
                rdfs:subPropertyOf foaf:name .

gtfs:routeType a rdf:Property ;
               rdfs:domain gtfs:Route ;
               rdfs:range  xsd:nonNegativeInteger .

gtfs:agency  a rdf:Property ;
             rdfs:domain gtfs:Route ; 
             rdfs:range  gtfs:Agency .

# GTFS table "trips.txt"
# https://developers.google.com/transit/gtfs/reference#trips_fields
# gtfs:Trip a rdfs:Class .
# tripId dcterms:identifier
# serviceId
# tripHeadsign: gtfs:headsign
# tripShortName: gtfs:shortName
# directionId
# blockId
# shapeId

gtfs:service a rdf:Property ;
               rdfs:range gtfs:Service .

gtfs:direction a rdf:Property ;
              rdfs:range xsd:nonNegativeInteger .


gtfs:block a rdf:Property ;
           rdfs:range xsd:nonNegativeInteger .

gtfs:shape a rdf:Property ;
           rdfs:range xsd:nonNegativeInteger .


gtfs:route a rdf:Property;
           rdfs:domain gtfs:Trip ;
           rdfs:range  gtfs:Route .

gtfs:wheelchairAccessible a rdf:Property;
                          rdfs:domain gtfs:Trip ;
                          rdfs:range  xsd:boolean .
gtfs:bikesAllowed a rdf:Property;
                  rdfs:domain gtfs:Trip ;
                  rdfs:range  xsd:boolean .

### --- gtfs:StopTime class
gtfs:StopTime a rdfs:Class .

gtfs:trip a rdf:Property ;
          rdfs:range gtfs:Trip .
gtfs:stop a rdf:Property ;
          rdfs:range gtfs:Stop .

gtfs:arrivalTime a rdf:Property ;
                 rdfs:domain gtfs:StopTime .

gtfs:departureTime a rdf:Property ;
                   rdfs:domain gtfs:StopTime .

gtfs:stopSequence a rdf:Property ;
                  rdfs:domain gtfs:StopTime ;
                  rdfs:range xsd:nonNegativeInteger .

gtfs:headsign a rdf:Property ;
              rdfs:range xsd:string .

gtfs:pickupType a rdf:Property .

gtfs:dropOffType a rdf:Property .

gtfs:shapeDistTraveled a rdf:Property .

# --- calendar.txt and calendar_data.txt
gtfs:Service a rdfs:Class .

gtfs:mondaya a rdf:Property ;
             rdfs:range xsd:boolean .

gtfs:tuesday a rdf:Property ;
             rdfs:range xsd:boolean .

gtfs:wednesday a rdf:Property ;
               rdfs:range xsd:boolean .

gtfs:thursday a rdf:Property ;
              rdfs:range xsd:boolean .

gtfs:friday a rdf:Property ;
            rdfs:range xsd:boolean .

gtfs:saturday a rdf:Property ;
              rdfs:range xsd:boolean .

gtfs:sunday a rdf:Property ;
            rdfs:range xsd:boolean .

gtfs:startDate a rdf:Property .

gtfs:endDate a rdf:Property .

# --- gtfs:FareAttribute class
gtfs:FareAttribute a rdfs:Class .

# --- gtfs:FareRule class
gtfs:FareRule a rdfs:Class .

# GTFS table "shapes.txt". Note that this is normalized differently than the GTFS spec itself.
gtfs:Shape a rdfs:Class .
# has these predicates:

# GTFS table "shapes.txt". Note that this is normalized differently than the GTFS spec itself.
gtfs:ShapePoint a rdfs:Class .

# has these predicates:

# GTFS table "frequencies.txt"
# https://developers.google.com/transit/gtfs/reference#frequencies_fields
gtfs:Frequency a rdfs:Class .
# has these predicates:
gtfs:trip a rdf:Property ;
          rdfs:range gtfs:Trip .
# Not forbidden to repeat same triples elsewhere or add others with the same subject. tripId will also appear in TripTimes. 
gtfs:startTime a rdf:Property ;
               rdfs:domain gtfs:Frequency ;
               rdfs:range xsd:time .
gtfs:endTime a rdf:Property ;
             rdfs:domain gtfs:Frequency ;
             rdfs:range xsd:time .
gtfs:headwaySecs a rdf:Property;
                 rdfs:domain gtfs:Frequency ;
                 rdfs:range owl:Interval .
gtfs:exactTimes a rdf:Property;
                rdfs:domain gtfs:Frequency ;
                rdfs:range xsd:boolean .


# GTFS table "transfers.txt"
# https://developers.google.com/transit/gtfs/reference#transfers_fields
gtfs:Transfer a rdfs:Class .
# has these predicates:
gtfs:fromStop a rdf:Property ;
              rdfs:domain gtfs:Transfer ;
              rdfs:range gtfs:Stop .
gtfs:toStop a rdf:Property ;
            rdfs:domain gtfs:Transfer ;
            rdfs:range gtfs:Stop .
gtfs:transferType a rdf:Property ;
                  rdfs:domain gtfs:Transfer ;
                  rdfs:range xsd:nonNegativeInteger .
gtfs:minTransferTime a rdf:Property ;
                     rdfs:domain gtfs:Transfer ;
                     rdfs:range owl:Interval .

